/*
 *  算法思想：本题是动态规划问题，状态转移方程：array[i] = array[i-1]+array[i-2];
*/
class Solution {
public:
    int climbStairs(int n) {
        if (n == 1)
            return 1;
        else if (n == 2)
            return 2;
        vector<int> array(n);
        array[0] = 1;
        array[1] = 2;
        for (int i=2; i<n; i++) {
            array[i] = array[i-1]+array[i-2];
        }
        return array[n-1];
    }
};
